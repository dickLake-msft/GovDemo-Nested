{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "rg-name":{
            "type":"string"
        }
    },
    "functions": [],
    "variables": {
        "FrontDoorProfileName": "[concat(parameters('rg-name'), '-NS-FD')]",
        "FrontDoorEndpointName": "[concat(parameters('rg-name'), '-NS-FD-Endpoint')]",
        "FrontDoorOriginGroupName": "[concat(parameters('rg-name'), '-NS-FD-OWASP')]",
        "FrontDoorRouteName": "[concat(parameters('rg-name'), '-NS-FD-APPGw')]",
        "FrontDoorOriginName": "[concat(parameters('rg-name'), '-NS-FD-OWASP-APPGW')]",
        "FrontDoorWAFName": "[concat(parameters('rg-name'), 'NSFDWAF')]",
        "FrontDoorSecPolName": "[concat(parameters('rg-name'), '-NS-FD-SecPol')]"
    },
    "resources": [
        {
            "type": "Microsoft.Network/frontdoorwebapplicationfirewallpolicies",
            "apiVersion": "2020-11-01",
            "name": "[variables('FrontDoorWAFName')]",
            "location": "global",
            "sku": {
                "name": "Premium_AzureFrontDoor"
            },
            "properties": {
                "policySettings": {
                    "enabledState": "Enabled",
                    "mode": "Prevention",
                    "redirectUrl": "https://www.microsoft.com/en-us/edge?r=1",
                    "customBlockResponseStatusCode": 403,
                    "customBlockResponseBody": "[base64('<html><head><title>WAF Demo</title></head><body bgcolor=\"#FFB299\"><p><h1><strong>WAF Unique Response Code</strong></h1></p><p>Contact us and provide the following code: {(azure-ref)}<br></p></body></html>')]",
                    "requestBodyCheck": "Enabled"
                },
                "customRules": {
                    "rules": [
                        {
                            "name": "BlockGeoLocationChina",
                            "enabledState": "Enabled",
                            "priority": 100,
                            "ruleType": "MatchRule",
                            "rateLimitDurationInMinutes": 1,
                            "rateLimitThreshold": 100,
                            "matchConditions": [
                                {
                                "matchVariable": "RemoteAddr",
                                "operator": "GeoMatch",
                                "negateCondition": false,
                                "matchValue": [
                                    "CN"
                                ],
                                "transforms": []
                                }
                            ],
                            "action": "Block"
                        },
                        {
                            "name": "RuleRedirectInternetExplorer",
                            "enabledState": "Enabled",
                            "priority": 200,
                            "ruleType": "MatchRule",
                            "rateLimitDurationInMinutes": 1,
                            "rateLimitThreshold": 100,
                            "matchConditions": [
                                {
                                "matchVariable": "RequestHeader",
                                "selector": "User-Agent",
                                "operator": "Contains",
                                "negateCondition": false,
                                "matchValue": [
                                    "rv:11.0"
                                ],
                                "transforms": []
                                }
                            ],
                            "action": "Redirect"
                        },
                        {
                            "name": "RatelimitRequest",
                            "enabledState": "Enabled",
                            "priority": 300,
                            "ruleType": "RateLimitRule",
                            "rateLimitDurationInMinutes": 1,
                            "rateLimitThreshold": 5,
                            "matchConditions": [
                                {
                                "matchVariable": "RequestUri",
                                "operator": "Contains",
                                "negateCondition": false,
                                "matchValue": [
                                    "search"
                                ],
                                "transforms": []
                                }
                            ],
                            "action": "Block"
                        }
                    ]
                },
                "managedRules": {
                    "managedRuleSets": [
                        {
                        "ruleSetType": "Microsoft_DefaultRuleSet",
                        "ruleSetVersion": "1.1"
                        },
                        {
                        "ruleSetType": "Microsoft_BotManagerRuleSet",
                        "ruleSetVersion": "1.0"
                        }
                    ]
                }
            }
        },
        {
            "apiVersion": "2020-05-01",
            "type": "Microsoft.Network/frontdoors",
            "name": "[variables('FrontDoorProfileName')]",
            "location": "usgovvirginia",
            "tags": {},
            "properties": {
                "friendlyName": "[concat(parameters('rg-name'), '-FrontDoor')]",
                "enabledState": "Enabled",
                "healthProbeSettings": [
                    {
                        "name": "healthProbeSettings-1653604895866",
                        "properties": {
                            "path": "/",
                            "protocol": "Https",
                            "intervalInSeconds": 30,
                            "healthProbeMethod": "Head",
                            "enabledState": "Enabled"
                        }
                    }
                ],
                "loadBalancingSettings": [
                    {
                        "name": "loadBalancingSettings-1653604895866",
                        "properties": {
                            "sampleSize": 4,
                            "successfulSamplesRequired": 2,
                            "additionalLatencyMilliseconds": 0
                        }
                    }
                ],
                "frontendEndpoints": [
                    {
                        "name": "ContosoGov-FD-frontEndEndpoint",
                        "properties": {
                            "hostName": "asdfgtest.azurefd.us",
                            "sessionAffinityEnabledState": "Enabled",
                            "sessionAffinityTtlSeconds": 0,
                            "webApplicationFirewallPolicyLink": {
                                "id":"[resourceId('Microsoft.Network/frontdoorwebapplicationfirewallpolicies',variables('FrontDoorWAFName'))]",
                            },
                            "customHttpsConfiguration": null
                        }
                    }
                ],
                "backendPools": [
                    {
                        "name": "ContosoGov-FD-backendPool1",
                        "properties": {
                            "backends": [
                                {
                                    "address": "52.244.54.210",
                                    "enabledState": "Enabled",
                                    "httpPort": 80,
                                    "httpsPort": 443,
                                    "priority": 1,
                                    "weight": 50,
                                    "backendHostHeader": "52.244.54.210"
                                }
                            ],
                            "loadBalancingSettings": {
                                "id": "/subscriptions/04110ca7-4fe3-4c2d-9ff7-23aa92b86c79/resourceGroups/ContosoGov-WebApps-RG/providers/Microsoft.Network/frontdoors/asdfg/loadBalancingSettings/loadBalancingSettings-1653604895866"
                            },
                            "healthProbeSettings": {
                                "id": "/subscriptions/04110ca7-4fe3-4c2d-9ff7-23aa92b86c79/resourceGroups/ContosoGov-WebApps-RG/providers/Microsoft.Network/frontdoors/asdfg/healthProbeSettings/healthProbeSettings-1653604895866"
                            }
                        },
                        "id": "/subscriptions/04110ca7-4fe3-4c2d-9ff7-23aa92b86c79/resourceGroups/ContosoGov-WebApps-RG/providers/Microsoft.Network/frontdoors/asdfg/backendPools/asdfg-backend"
                    }
                ],
                "routingRules": [
                    {
                        "name": "asdfg-routingrule",
                        "properties": {
                            "frontendEndpoints": [
                                {
                                    "id": "/subscriptions/04110ca7-4fe3-4c2d-9ff7-23aa92b86c79/resourceGroups/ContosoGov-WebApps-RG/providers/Microsoft.Network/frontdoors/asdfg/frontendEndpoints/asdfg-azurefd-us"
                                }
                            ],
                            "acceptedProtocols": [
                                "Http",
                                "Https"
                            ],
                            "patternsToMatch": [
                                "/*"
                            ],
                            "enabledState": "Enabled",
                            "routeConfiguration": {
                                "@odata.type": "#Microsoft.Azure.FrontDoor.Models.FrontdoorForwardingConfiguration",
                                "forwardingProtocol": "HttpsOnly",
                                "backendPool": {
                                    "id": "/subscriptions/04110ca7-4fe3-4c2d-9ff7-23aa92b86c79/resourceGroups/ContosoGov-WebApps-RG/providers/Microsoft.Network/frontdoors/asdfg/backendPools/asdfg-backend"
                                },
                                "cacheConfiguration": null
                            }
                        },
                        "id": "/subscriptions/04110ca7-4fe3-4c2d-9ff7-23aa92b86c79/resourceGroups/ContosoGov-WebApps-RG/providers/Microsoft.Network/frontdoors/asdfg/routingRules/asdfg-routingrule"
                    }
                ],
                "backendPoolsSettings": {
                    "enforceCertificateNameCheck": "Enabled",
                    "sendRecvTimeoutSeconds": 30
                }
            }
        }
    ],
    "outputs": {}
}